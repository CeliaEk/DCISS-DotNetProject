@model IEnumerable<DotNetSportAsso.Models.lieu>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.lieu_nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.adresse_lieu)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.lieu_nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.adresse_lieu)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.lieu_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.lieu_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.lieu_id })
        </td>
    </tr>
}

</table>
